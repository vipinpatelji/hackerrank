int substrings(char* n) {
    const int MOD = 1000000007;
    int len = strlen(n);

    long long totalSum = 0;
    long long sumEndingAt = 0;

    for (int i = 0; i < len; i++) {
        int digit = n[i] - '0';
        sumEndingAt = (sumEndingAt * 10 + (long long)(i + 1) * digit) % MOD;
        totalSum = (totalSum + sumEndingAt) % MOD;
    }

    return (int)totalSum;
    

}
