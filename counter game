#include <stdio.h>
int main() {
    int t; scanf("%d", &t);
    while (t--) {
        unsigned long n; scanf("%lu", &n);
        int cnt = 0;
        while (n > 1) {
            if ((n & (n - 1)) == 0) n >>= 1;
            else n -= 1UL << (63 - __builtin_clzl(n));
            cnt++;
        }
        puts(cnt % 2 ? "Louise" : "Richard");
    }
}
